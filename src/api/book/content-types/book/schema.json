{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Book",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Year": {
      "type": "integer",
      "required": true
    },
    "Synopsis": {
      "type": "text"
    },
    "Publisher": {
      "type": "string"
    },
    "Thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "BookCovers": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "BillboardImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "BillboardImageMobile": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "FooterImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Locations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::location.location",
      "inversedBy": "books"
    },
    "Characters": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::character.character",
      "inversedBy": "books"
    },
    "Genres": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::genre.genre",
      "mappedBy": "books"
    },
    "slug": {
      "type": "uid",
      "targetField": "Title",
      "required": true
    }
  }
}
